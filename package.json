  "scripts": {
    "build:developer": "echo \"Building code...\" && npm run clean && npm run copy-static-assets && npm run webpack",
    "build:production": "echo \"Building code...\" && npm run clean && npm run copy-static-assets && npm run webpack:production",
    "build:watch": "echo \"Building code...\" && npm run clean && npm run copy-static-assets && npm run webpack:watch",
    "clean": "echo \"Cleaning out build directory...\" && rm -rf ./web/build",
    "copy-static-assets": "echo \"Copying static assets...\" && mkdir -p ./web/build/images && cp -R ./web/app/images/ ./web/build/images",
    "lint": "echo \"Linting javascript code...\" && ./node_modules/.bin/eslint --config prod.eslintrc src/ test/",
    "lint:fix": "echo \"Auto formating javascript code...\" && ./node_modules/.bin/prettier-eslint --write \"src/**/*.js\" \"test/**/*.js\"",
    "precommit": "yarn lint:fix && yarn lint",
    "prepush": "yarn lint && yarn test",
    "prepublish": "rm -rf ./lib && mkdir -p ./lib/styles && ./node_modules/babel-cli/bin/babel.js src --out-dir lib && cp -r ./src/styles/ ./lib/styles",
    "test": "NODE_PATH=. NODE_ENV=test ./node_modules/.bin/ava --verbose",
    "test:watch": "NODE_PATH=. NODE_ENV=test ./node_modules/.bin/ava --verbose --watch",
    "test:update": "NODE_PATH=. NODE_ENV=test ./node_modules/.bin/ava --verbose --update-snapshots",
    "webpack": "yarn clean && yarn && NODE_ENV=development ./node_modules/.bin/webpack --progress --config webpack.config.js",
    "webpack:production": "yarn clean && yarn && NODE_ENV=production WEBPACK_PRODUCTION_BUILD=1 ./node_modules/.bin/webpack --progress --config webpack.config.js",
    "webpack:watch": "yarn clean && yarn && NODE_ENV=development ./node_modules/.bin/webpack --progress --config webpack.config.js --watch"
  },
  "version": "0.1.0"
}
