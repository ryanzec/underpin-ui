{
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "node",
    "module": "es2015",
    "sourceMap": true,
    "noImplicitAny": true,
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "src/*": ["./src/*"],
      "app/*": ["./web/app/*"]
    }
  },
  //"include": ["./src/**/*", "./web/app/**/*"],
  "exclude": ["node_moudles", "web/build", "lib"]
}
